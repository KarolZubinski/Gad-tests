    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.1",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
